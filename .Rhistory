install.packages("shiny")
library("shiny")
library(shinydashboard)
library(ggplot2)
library(dplyr)
rec = read.csv("C:/Users/student/Downloads/SBI.txt")
rec
header <- dashboardHeader(title =  "CMP Dashboard")
header
sidebar = dashboardSidebar(
sidebarMenu(
menuItem(title = "Dashboard",tabname = "dashboard",icon = icon("dashboard")),
menuItem(title = "visit-us",icon = icon("send",lib = "glyphicon"),href = "https://www.salesforce.com")
)
)
frow1 = fluidRow(
valueBoxOutput("profAcc"),
valueBoxOutput("marketShare"),
alueBoxOutput("profitMargin")
)
frow2 = fluidRow(
box(
title = "Revenue per Account",
status = "primary",
solidHeader = TRUE,
collapsible = TRUE,
plotOutput("salesQuarterBar",height = "300px")
),
box(
title = "Revenue per Account",
status = "primary",
solidHeader = TRUE,
collapsible = TRUE,
plotOutput("salesYearBar",height = "300px")
)
)
frow3 = fluidRow(
box1(
title = "Revenue per Region",
status = "primary",
solidHeader = TRUE,
collapsible = TRUE,
plotOutput("shareLine",height = "300px")
),
box2(
title = "Revenue per Product",
status = "primary",
solidHeader = TRUE,
collapsible = TRUE,
plotOutput("shareBar",height = "300px")
),
)
frow4 = fluidRow(
tabBox(
title = "Revenue1",
width = 10,
id = "dataTabBox",
tabPanel1(title = "Revenue1",dataTableOutput("salesbymodel")
),
tabPanel2(
title = "Revenue2",dataTableOutput("salesbyQuarter")
),
tabPanel3(
title = "Revenue3",dataTableOutput("salesbyYear")
)
)
)
body = dashboardBody(frow1,frow2,frow3,frow4)
ui = dashboardPage(header,sidebar,body,skin='red')
server =function(input,output){
total.revenue = sum(rec$Revenue)
total.account = rec%>% group_by(Account)%>% summarise(value = sum(Revenue)%>% filter(value == max(value)))
total.product = rec%>%group_by(Product)%>% summarise(value = sum(Revenue)%>% filter(value == max(value)))
}
output$profAcc = renderValueBox({
valueBox(
formatC(prof.account$value,format = "d",big.mark = ','),
paste("Top Account:",prof.account$Account),
icon = icon("stats",lib = 'glyphicon'),
color = 'purple'
)
})
output$marketShare = renderValueBox({
valueBox(
formatC(prof.account$value,format = "d",big.mark = ','),'Total Expected Revenue',
icon = icon("stats",lib = 'glyphicon'),
color = "green"
)
})
output$profitMargin = renderValueBox({
valueBox(
formatC(prof.prod$value,format = "d",big.mark = ','),
paste("Top Product:",prof.prod$Product),
icon = icon("menu-hamburger",lib = 'glyphicon'),
color = "yellow"
)
})
output$salesQuartBar = renderPlot({
ggplot(data = rec,
aes(x = Product,y = Revenue,fill = factor(Region)))+
geom_bar(position = "dodge",stat = "identity")+ ylab("Revenue (in Euros")+
xlab("Product")+theme(legend.position = "bottom",plot.title = element_text(size = 15,face = "bold"))+
ggtitle("Revenue by Product")+labs(fill = "Region")
})
